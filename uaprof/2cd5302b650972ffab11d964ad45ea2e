<!--Data layer js code,social track,url params filter-->
<script>
var dataLayer = dataLayer || [];
var _dl = _dl || {};

_dl.trackSocial = function(opt_pageUrl, opt_trackerName) {
  _dl.trackFacebook(opt_pageUrl, opt_trackerName);
  _dl.trackTwitter(opt_pageUrl, opt_trackerName);
  _dl.trackVK(opt_pageUrl, opt_trackerName);
};

_dl.trackFacebook = function(opt_pageUrl, opt_trackerName) {
  var trackerName = _dl.buildTrackerName_(opt_trackerName);
  try {
    if (FB && FB.Event && FB.Event.subscribe){
        FB.Event.subscribe('edge.create', function(targetUrl) {
          dataLayer.push({'event' : 'track-social', 'socialNetwork':'Facebook', 'socialAction' : 'Like','socialTarget':targetUrl});
        });
        FB.Event.subscribe('edge.remove', function(targetUrl) {
          dataLayer.push({'event' : 'track-social', 'socialNetwork':'Facebook', 'socialAction' : 'Unlike','socialTarget':targetUrl});
        });
      }
  } catch(e){}
};
// vk track
_dl.trackVK = function(opt_pageUrl, opt_trackerName, opt_targetUrl) {
  var trackerName = _dl.buildTrackerName_(opt_trackerName);
  try{
        if (VK && VK.Observer && VK.Observer.subscribe) {
            VK.Observer.subscribe('widgets.like.liked', function(targetUrl) {
              dataLayer.push({'event' : 'track-social', 'socialNetwork':'vk.com', 'socialAction': 'Like','socialTarget':targetUrl});
            });
            VK.Observer.subscribe('widgets.like.unliked', function(targetUrl) {
              dataLayer.push({'event' : 'track-social', 'socialNetwork':'vk.com', 'socialAction': 'Unlike','socialTarget':targetUrl});
            });
    }
  } catch (e) {}
};
//track name function
_dl.buildTrackerName_ = function(opt_trackerName) {
  return opt_trackerName ? opt_trackerName + '.' : '';
};
//twitter track
_dl.trackTwitter = function(opt_pageUrl, opt_trackerName) {
  var trackerName = _dl.buildTrackerName_(opt_trackerName);
  try {
    if (twttr && twttr.events && twttr.events.bind) {
      twttr.events.bind('tweet', function(event) {
        if (event) {
          var targetUrl; // Default value is undefined.
          if (event.target && event.target.nodeName == 'IFRAME') {
            targetUrl = _dl.extractParamFromUri_(event.target.src, 'url');
          }
          dataLayer.push({'event': 'track-social', 'socialNetwork':'Twitter', 'socialAction':'Tweet', 'socialTarget': targetUrl });
        }

      });
    }
  } catch (e) {}
};
//get url params
_dl.extractParamFromUri_ = function(uri, paramName) {
  if (!uri) {
    return;
  }
  var uri = uri.split('#')[0];  // Remove anchor.
  var parts = uri.split('?');  // Check for query params.
  if (parts.length == 1) {
    return;
  }
  var query = decodeURI(parts[1]);
  // Find url param.
  paramName += '=';
  var params = query.split('&');
  for (var i = 0, param; param = params[i]; ++i) {
    if (param.indexOf(paramName) === 0) {
      return unescape(param.split('=')[1]);
    }
  }
  return;
};
    //legacy code was here, mover to footer
    // tag #head_legacy
</script>

