<script type="text/javascript">

function compare_past(time) {
  var currentStatus = false;
  var past = time;
  past = past.split("/");
  var curdate = new Date();
  curdate = curdate.toLocaleDateString();
  curdate = curdate.split("/");
  //console.log(curdate);
  if (past[2]<curdate[2]) {
    //cur year is bigger
    currentStatus=true;
  }
  if (past[1]<curdate[1]) {
    if (past[2]==curdate[2])
    currentStatus=true;
    //cur month is bigger
  }
  if (past[0]<curdate[0]) {
    if (past[2]==curdate[2] && past[1] == curdate[1]) currentStatus=true;
    //cur day is bigger
  }
  return currentStatus;
}

function setCookie(cname,cvalue,exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires=" + d.toGMTString();
    document.cookie = cname+"="+cvalue+"; "+expires+";domain=.fly-phone.ru;path=/";
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) === 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}
$(document).ready(function(){
  var saveStatus = getCookie("lkbanner_status");
  if (saveStatus=="already") {
    //don't show
    $(".wrapp_lkpopupbanner").hide();
  }
  else {
    setTimeout(function() {$(".wrapp_lkpopupbanner").show();}, 5000);
  }
  $(".lkpopupbanner_close").click(function(event){
    event.preventDefault();
    $(".wrapp_lkpopupbanner").hide();
    /*var curdate = new Date();
    curdate = curdate.toLocaleDateString();
    curdate = curdate.split("/");
    curdate[1]=parseInt(curdate[1])+1;
    if (curdate[1]<10) curdate[1]="0"+curdate[1];
    curdateSTR=curdate[0]+"/"+curdate[1]+"/"+curdate[2];*/
    setCookie("lkbanner_status","already",1);
    //set cookie after
  });
  $(".lkpopupbanner_hide").click(function(event){
     event.preventDefault();
     $(".wrapp_lkpopupbanner").hide();
     setCookie("lkbanner_status","already",730);
  })
  //compare_past("02/02/2015");
})
</script>
