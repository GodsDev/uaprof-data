    </rdf:Description>      
  </prf:component>
    
  <!-- NetworkCharacteristics -->
  <prf:component>
    <rdf:Description rdf:ID="NetworkCharacteristics">
      <rdf:type rdf:resource="http://www.openmobilealliance.org/tech/profiles/UAPROF/ccppschema-20021212#NetworkCharacteristics" />   
