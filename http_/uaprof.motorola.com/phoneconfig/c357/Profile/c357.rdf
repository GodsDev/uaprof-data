        <rdf:li>phone:ringtone</rdf:li>
       </rdf:Bag>
      </prf:MmcRingtone>
      <prf:MmcWallpaper>
       <rdf:Bag>
        <rdf:li>phone:wallpaper</rdf:li>
       </rdf:Bag>
      </prf:MmcWallpaper>
      <prf:CcppAccept-Charset>
       <rdf:Bag>
        <rdf:li>UTF-8</rdf:li> 
        <rdf:li>ISO-8859-1</rdf:li> 
        <rdf:li>US-ASCII</rdf:li> 
        <rdf:li>ISO-10646-UCS-2</rdf:li> 
       </rdf:Bag>
      </prf:CcppAccept-Charset>
      <prf:CcppAccept-Encoding>
       <rdf:Bag>
        <rdf:li>None</rdf:li>
       </rdf:Bag>
      </prf:CcppAccept-Encoding>
      <prf:CcppAccept-Language>
       <rdf:Seq>
        <rdf:li>en</rdf:li> 
        <rdf:li>pt</rdf:li>
        <rdf:li>sp</rdf:li>
       </rdf:Seq>
      </prf:CcppAccept-Language>
    </rdf:Description>
   </prf:component>

