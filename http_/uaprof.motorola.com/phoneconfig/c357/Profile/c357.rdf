        <rdf:li>1.2.1</rdf:li>
       </rdf:Bag>
      </prf:WmlScriptVersion>
      <prf:WmlScriptLibraries>
       <rdf:Bag>
        <rdf:li>Lang</rdf:li>
        <rdf:li>Float</rdf:li>
