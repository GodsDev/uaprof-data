      <prf:WapVersion>2.0</prf:WapVersion>
      <prf:WmlVersion>
       <rdf:Bag>
        <rdf:li>1.1</rdf:li>
        <rdf:li>1.2</rdf:li>
        <rdf:li>1.3</rdf:li>
       </rdf:Bag>
      </prf:WmlVersion>
      <prf:WmlDeckSize>7700</prf:WmlDeckSize>
      <prf:WmlScriptVersion>
